{
  "extends": ["eslint:recommended", "airbnb"],
  "rules": {
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "no-empty": 0,
    "no-else-return": 0,
    "no-unused-expressions": 0,
    "no-plusplus": 0,
    "func-names": 0,
    "prefer-template": 0,
    "import/no-extraneous-dependencies": [0, {"devDependencies": ["test/*.spec.js"]}]
  }
}
