{
  "name": "babel-npm-boilerplate",
  "author": "Cam Song",
  "version": "0.2.0",
  "description": "A Boilerplate for creating NPM packages using Babel, ESLint, Mocha and Chai",
  "main": "build/index.js",
  "keywords": [
    "es6",
    "babel",
    "eslint",
    "npm package"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bin/_mocha",
    "build": "babel src --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src test"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.10.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camsong/babel-npm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/camsong/babel-npm-boilerplate/issues"
  },
  "homepage": "https://github.com/camsong/babel-npm-boilerplate#readme",
  "license": "MIT"
}
