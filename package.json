{
  "name": "babel-npm-boilerplate",
  "author": "Cam Song",
  "version": "0.0.1",
  "description": "A Boilerplate for creating NPM packages using Babel, ESLint, Mocha and Chai",
  "main": "build/index.js",
  "keywords": [
    "es6",
    "babel",
    "eslint",
    "npm package"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "chai": "^3.2.0",
    "eslint": "^1.1.0",
    "eslint-config-airbnb": "^0.0.7",
    "eslint-plugin-react": "^3.2.1",
    "mocha": "^2.2.5"
  },
  "scripts": {
    "test": "mocha --compilers js:babel/register --recursive --ui bdd --reporter spec",
    "build": "babel src/ --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camsong/fetch-jsonp.git"
  },
  "bugs": {
    "url": "https://github.com/camsong/fetch-jsonp/issues"
